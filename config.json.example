{
  "telegram_bot_token": "YOUR_TELEGRAM_BOT_TOKEN", // Токен Telegram бота, полученный от @BotFather
  "telegram_bot_admin_ids": [YOUR_ADMIN_TELEGRAM_ID], // Список Telegram ID администраторов (можно указать несколько). Для получения ID используйте бота @userinfobot
  "higgsfield_api_key": "YOUR_HIGGSFIELD_API_KEY", // API ключ для доступа к платформе Higgsfield AI
  "higgsfield_api_key_secret": "YOUR_HIGGSFIELD_API_KEY_SECRET", // Секретный ключ API для платформы Higgsfield AI
  "higgsfield_api_url": "https://cloud.higgsfield.ai/api", // Базовый URL API платформы Higgsfield (обычно не требуется изменять)
  "higgsfield_model_id": "bytedance/seedream/v4/text-to-image", // ID модели для генерации (обычно не требуется изменять)
  "deepseek_api_key": "YOUR_DEEPSEEK_API_KEY", // API ключ для DeepSeek (используется для промпт-мастера). Получить можно на https://platform.deepseek.com/
  "admin_password": "YOUR_ADMIN_PASSWORD", // Пароль для доступа к веб-админ-панели (Flask)
  "storage_path": "storage/users", // Путь к директории хранения файлов пользователей (относительно корня проекта)
  "database_path": "database/app.db", // Путь к файлу базы данных SQLite (относительно корня проекта)
  "flask_host": "0.0.0.0", // IP-адрес для Flask сервера (0.0.0.0 - слушать на всех интерфейсах)
  "flask_port": 5000, // Порт для Flask сервера админ-панели
  "log_level": "INFO", // Уровень логирования: DEBUG, INFO, WARNING, ERROR
  "log_file": "logs/app.log", // Путь к файлу логов (относительно корня проекта)
  "max_file_size": 10485760, // Максимальный размер загружаемого файла в байтах (по умолчанию 10 МБ)
  "api_generation_timeout": 300, // Таймаут ожидания генерации изображения в секундах (по умолчанию 5 минут)
  "file_cache_ttl_days": 7 // Время жизни кэша ссылок на загруженные файлы в днях (по умолчанию 7 дней)
}
